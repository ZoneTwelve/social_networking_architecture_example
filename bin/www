#!/usr/bin/env node
var app = require('../app');
app.set('port', 80);
app.set('httpsport', 443);
var ssl = require('../SSL.js');
var http = require('http');
var https = require('https');
var httpServer = http.createServer(app);
var httpsServer = https.createServer(ssl.options, app);
httpServer.listen(app.get('port'));
httpsServer.listen(app.get('httpsport'));
var socket = require('socket.io')(httpsServer);
var socketServer = require('../websocket.js')(socket);